{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "installCommand": "npm install",
  "framework": "create-react-app",
  "env": {
    "REACT_APP_API_BASE_URL": "https://todo-backend-ja58.onrender.com/api",
    "REACT_APP_SOCKET_URL": "https://todo-backend-ja58.onrender.com",
    "REACT_APP_ENV": "production"
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
